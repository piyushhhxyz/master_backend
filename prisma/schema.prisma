generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id        Int      @id @default(autoincrement())
  name      String   @db.VarChar(191)
  email     String   @unique @db.VarChar(191)
  password  String
  profile   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
